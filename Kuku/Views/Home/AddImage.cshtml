@model IEnumerable<Kuku.Models.OriginalImage>

@{
    ViewBag.Title = "Добавление изображения";
}
<form asp-action="AddImage" asp-controller="Home" method="post" enctype="multipart/form-data">

    <p>
        <label>Выберите изображение</label>
        <input name="uploadedFile" type="file" class="form-control" />
    </p>
    <p>
        <input type="submit" value="Добавить изображение" />
    </p>
</form>
<h3>Все изображения</h3>
@foreach (var p in Model)
{
    <div>
        <h4>@p.FileName</h4>
        @if (p.OriginalImageData != null)
        {
            <img style='width:320px; height:240px;' src="data:image/jpeg;base64,@(Convert.ToBase64String(p.OriginalImageData))" />
        }
    </div>
}